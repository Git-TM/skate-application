<%= link_to skateshops_path(skateshop.id), class: 'info-links' do %>
  <div class='info-div'>
    <h4><%= skateshop.name %></h4>
    <p><%= skateshop.address %> </p>
    <% if skateshop.photos.attached? %>
      <% skateshop.photos.each do |illustration| %>
          <%= image_tag(illustration, class: 'photo-info') %>
      <% end %>
     <% end %>
  </div>
<% end %>
